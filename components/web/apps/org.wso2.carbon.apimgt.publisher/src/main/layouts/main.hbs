<!DOCTYPE html>
<html lang="en">
<head>
    <script>
        var requestURL = '/publisher';
        var ssoEnabled = 'false';
        var tenantDomain = 'null';
    </script>
    <title>{{#placeholder "title"}}API Publisher{{/placeholder}}</title>
    <meta charset="UTF-8">
    <meta name="description" content=""/>
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
    <!-- Le fav and touch icons -->
    {{#placeholder "favicon"}}
        <link rel="shortcut icon" href="{{public "images/default-favicon.png"}}" type="image/png" />
    {{/placeholder}}
    {{placeholder "css"}}
    {{placeholder "headJs"}}
    {{placeholder "headOther"}}
</head>
<body class="sticky-footer ">
<!-- header -->
{{defineZone "topbar"}}

<!-- End header -->
<!--Breadcumb -->
<div class="breadcrumb-wrapper">
    <ol class="breadcrumb">
        <li>
            <a href="/publisher/site/pages/index.jag" title="Home" onclick="jagg.sessionAwareJS({redirect:'/publisher/site/pages/index.jag',e:event})"><i class="icon fw fw-home"></i> <span class="hidden-xs">Home</span></a>
        </li>
        <li class="active">
            <a href="/publisher/apis" title="API List" onclick="jagg.sessionAwareJS({redirect:'/publisher/apis',e:event})">APIs</a>
        </li>
    </ol>
</div>

<!--End Breadcumb -->
<!-- #page-content-wrapper -->
<div class="page-content-wrapper" style="padding-left: 300px;">
    {{defineZone "primaryMenu"}}
    {{defineZone "secondaryMenu"}}


    <!-- page content -->
    <div class="container-fluid">
        <div class="body-wrapper">
            {{defineZone "content"}}
        </div>
    </div>
</div>
<!-- /#wrapper -->
<footer class="footer">
    <div class="container-fluid">
        {{defineZone "footer"}}    </div>
</footer>
{{placeholder "js"}}
</body>
</html>